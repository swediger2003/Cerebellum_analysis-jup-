    "def string_to_tuple(string):\n",
    "    list = []\n",
    "    entry = ''\n",
    "    for char in string:\n",
    "        if char in (',', ')'):\n",
    "            list.append(float(entry))\n",
    "            entry = ''\n",
    "        elif char not in ['(', ')']:\n",
    "            entry += char\n",
    "    if entry != '':\n",
    "        list.append(float(entry))\n",
    "    return tuple(list)"
   ]
